triggeremu_MC_config_icarus.settings.PMTADCthresholds                        : @local::triggeremu_config_icarus.settings.PMTADCthresholds
triggeremu_MC_config_icarus.settings.LVDSsignalWidth                         : @local::triggeremu_config_icarus.settings.LVDSsignalWidth
triggeremu_MC_config_icarus.settings.trigger_channel_pairings_nosingle       : @local::triggeremu_config_icarus.settings.trigger_channel_pairings_nosingle
